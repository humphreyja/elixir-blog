<header>
  <div class="container">
    <div class="website-title">
      General Knowledge
    </div>
    <div class="website-nav">
      <span>More</span>
      <nav>
        <%= if @signed_in do %>
          <%= link "Sign Out", to: sign_out_path(@conn, :delete) %>
        <% else %>
          <%= link "Sign In", to: sign_in_path(@conn, :new) %>
        <% end %>
      </nav>
    </div>
  </div>

</header>
